    "keywords": ["tmdb", "api", "php","wrapper", "movie", "tv", "tv show", "tvdb", "symfony", "symfony2"],
    "authors": [
        {
            "name": "Michael Roterman",
            "homepage": "http://wtfz.net",
            "email": "michael@wtfz.net"
        }
    ],
    "require": {
        "symfony/symfony": "~2.3",
        "php-tmdb/api": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "monolog/monolog": ">=1.11.0"
    },
    "autoload": {
        "psr-4": { "Tmdb\\SymfonyBundle\\": "" }
    }
}
